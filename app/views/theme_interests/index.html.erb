<p id="notice"><%= notice %></p>

<h1>Theme Interests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @theme_interests.each do |theme_interest| %>
      <tr>
        <td><%= theme_interest.name %></td>
        <td><%= theme_interest.description %></td>
        <td><%= link_to 'Show', theme_interest %></td>
        <td><%= link_to 'Edit', edit_theme_interest_path(theme_interest) %></td>
        <td><%= link_to 'Destroy', theme_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Theme Interest', new_theme_interest_path %>
